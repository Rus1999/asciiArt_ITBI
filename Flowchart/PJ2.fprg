            <assign variable="charac[6]" expression="1"/>
            <assign variable="charac[7]" expression="1"/>
            <assign variable="charac[8]" expression="1"/>
            <assign variable="charac[9]" expression="1"/>
            <assign variable="charac[10]" expression="1"/>
            <assign variable="charac[11]" expression="1"/>
            <assign variable="charac[12]" expression="2"/>
            <assign variable="charac[13]" expression="1"/>
            <assign variable="charac[14]" expression="0"/>
            <assign variable="charac[15]" expression="1"/>
            <assign variable="charac[16]" expression="1"/>
            <assign variable="charac[17]" expression="1"/>
            <assign variable="charac[18]" expression="1"/>
            <assign variable="charac[19]" expression="1"/>
            <assign variable="charac[20]" expression="3"/>
            <assign variable="charac[21]" expression="1"/>
            <assign variable="charac[22]" expression="3"/>
            <assign variable="charac[23]" expression="3"/>
            <assign variable="charac[24]" expression="0"/>
            <comment text=""/>
            <declare name="sp" type="Integer" array="False" size=""/>
            <assign variable="sp" expression="0"/>
            <declare name="ch" type="Integer" array="False" size=""/>
            <assign variable="ch" expression="0"/>
            <while expression="sp&lt;(space[col]*userInput)">
                <output expression="&quot;C&quot;" newline="False"/>
                <assign variable="sp" expression="sp+1"/>
            </while>
            <while expression="ch&lt;(charac[col]*userInput)">
                <output expression="&quot;W&quot;" newline="False"/>
                <assign variable="ch" expression="ch+1"/>
            </while>
            <assign variable="sp" expression="0"/>
            <assign variable="ch" expression="0"/>
            <if expression="col==4||col=9||col=14||col=19||col=24">
                <then>
                    <output expression="&quot;&quot;" newline="True"/>
                </then>
                <else/>
            </if>
        </body>
    </function>
