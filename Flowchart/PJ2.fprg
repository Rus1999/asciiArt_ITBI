    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <comment text="&#3605;&#3633;&#3623;&#3649;&#3611;&#3619;&#3619;&#3633;&#3610;&#3588;&#3656;&#3634;&#3592;&#3634;&#3585;&#3612;&#3641;&#3657;&#3651;&#3594;&#3657;&#13;&#10;"/>
            <declare name="userInput" type="Integer" array="False" size=""/>
